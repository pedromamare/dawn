<div id="w-node-_60575cb0-71b8-df62-c1b3-37dfdcf4cd63-5f3c681c" class="filters1_form-header">
  <div class="filters1_heading">Filtros</div>
  <div class="div-block-9">
    <a href="{{ collection.url }}" class="text-size-small color-brand">Limpar</a>
  </div>
</div>
<div class="filters1_filter-group-list">
  {%- for filter in collection.filters -%}
    <div class="filters1_filter-group">
      <div data-w-id="60575cb0-71b8-df62-c1b3-37dfdcf4cd90" class="filters1_filter-group-heading">
        <div class="text-size-medium-22 text-weight-semibold">{{ filter.label }}</div>
        <div bind="60575cb0-71b8-df62-c1b3-37dfdcf4cd93" class="filters1_accordion-icon w-embed"><svg width="100%" height="100%" viewbox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" clip-rule="evenodd" d="M12.3977 15.6629C12.178 15.8826 11.8219 15.8826 11.6022 15.6629L5.86739 9.92804C5.64772 9.70837 5.64772 9.35227 5.86739 9.13259L6.13256 8.86739C6.35222 8.64772 6.70838 8.64772 6.92805 8.86739L12 13.9393L17.0719 8.86739C17.2916 8.64772 17.6477 8.64772 17.8674 8.86739L18.1326 9.13259C18.3522 9.35227 18.3522 9.70837 18.1326 9.92804L12.3977 15.6629Z" fill="currentColor"></path>
          </svg></div>
      </div>
      <div style="height:0px" class="filters1_filter-options">
        <div class="w-form">
          <form>
            <div class="filters1_list-wrapper">
              <div class="filters1_list">
                {%- for value in filter.values -%}
                  <div class="filters1_item">
                    <label class="filters1_form-radio1 w-radio">
                      <input type="checkbox" name="{{ value.param_name }}" value="{{ value.value }}" class="w-form-formradioinput filters1_form-radio1-icon w-radio-input" {% if value.active %}checked{% endif %}>
                      <span class="filters1_form-radio1-label w-form-label" for="radio">{{ value.label }}</span>
                    </label>
                  </div>
                {%- endfor -%}
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  {%- endfor -%}
</div>