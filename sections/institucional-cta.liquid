<section class="section_cta5">
  <div data-w-id="6f30cf71-02f4-a967-a11e-5aca6d05bd40" data-is-ix2-target="1" class="lottie-magenta" data-animation-type="lottie" data-src="documents/lottieflow-background-14-d31f57-easey.json" data-loop="0" data-direction="1" data-autoplay="0" data-renderer="svg" data-default-duration="0" data-duration="8"></div>
  <div class="cta5_component">
