    </div>
    <div class="cta5_image-wrapper">
      {% if section.settings.image %}
        {{ section.settings.image | image_url: width: 1720 | image_tag: class: 'cta5_image', loading: 'lazy', sizes: '(max-width: 1720px) 100vw, 1720px' }}
      {% else %}
        <img sizes="(max-width: 1720px) 100vw, 1720px" src="{{ 'imgs-efeito03_1.avif' | asset_url }}" alt="" loading="lazy" class="cta5_image">
      {% endif %}
    </div>
  </div>
</section>

