      <h2 class="text-color-alternate">{{ section.settings.title | default: 'Lorem ipsum dolor sit amet, consectetur adipiscing elit.' }}</h2>
      <p class="text-color-alternate">{{ section.settings.text_1 | default: 'Lorem ipsum dolor sit amet, consectetur adipiscing elit. Suspendisse varius enim in eros elementum tristique.' }}</p>
